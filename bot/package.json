{
  "name": "eve-chart-bot",
  "version": "1.0.0",
  "description": "Discord bot for EVE Online kill and map activity charts",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "cli": "ts-node src/interfaces/cli/registry/index.ts",
    "test": "jest",
    "test:ingestion": "node --require ts-node/register src/scripts/test-ingestion.ts",
    "test:map-ingestion": "node --require ts-node/register src/scripts/test-map-ingestion.ts",
    "test:discord": "node --require ts-node/register src/scripts/test-discord.ts",
    "register:test": "node --require ts-node/register src/scripts/register-test-commands.ts",
    "create:groups": "ts-node src/scripts/create-default-groups.ts",
    "debug:groups": "ts-node src/scripts/debug-character-groups.ts",
    "fix:groups": "ts-node src/scripts/fix-character-groups.ts",
    "clean:groups": "ts-node src/scripts/clean-character-groups.ts",
    "cleanup-groups": "ts-node src/scripts/cleanup-empty-character-groups.ts",
    "verify:tables": "ts-node src/scripts/verify-tables.ts",
    "remove-character": "ts-node src/interfaces/cli/character-remove.ts",
    "db:reset": "ts-node src/interfaces/cli/registry/index.ts db reset --force",
    "db:migrate": "ts-node src/interfaces/cli/registry/index.ts db migrate",
    "db:migrate-map": "ts-node src/interfaces/cli/registry/index.ts db migrate-map-activity",
    "character:list": "ts-node src/interfaces/cli/registry/index.ts character list",
    "character:remove": "ts-node src/interfaces/cli/registry/index.ts character remove",
    "character:check-main": "ts-node src/interfaces/cli/registry/index.ts character check-main",
    "killmail:list": "ts-node src/interfaces/cli/registry/index.ts killmail list",
    "killmail:check": "ts-node src/interfaces/cli/registry/index.ts killmail check-ingestion",
    "discord:register": "ts-node src/interfaces/cli/registry/index.ts discord register-commands",
    "discord:check": "ts-node src/interfaces/cli/registry/index.ts discord check-commands",
    "diag:character-kills": "ts-node src/interfaces/cli/registry/index.ts diagnostic check-character-kills",
    "diag:kills-chart": "ts-node src/interfaces/cli/registry/index.ts diagnostic check-kills-chart",
    "diag:kill-ingestion": "ts-node src/interfaces/cli/registry/index.ts diagnostic check-kill-ingestion"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.20.0",
    "@sgratzl/chartjs-chart-boxplot": "^4.4.4",
    "@types/helmet": "^4.0.0",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "canvas": "^3.1.0",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-chart-matrix": "^2.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "class-transformer": "^0.5.1",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "node-cron": "^4.0.5",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/pino": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
