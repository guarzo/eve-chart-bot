{
  "name": "eve-chart-bot",
  "version": "1.0.0",
  "description": "Discord bot for EVE Online kill and map activity charts",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "cli": "ts-node src/interfaces/cli/index.ts",
    "test": "jest",
    "test:ingestion": "node --require ts-node/register src/scripts/test-ingestion.ts",
    "test:map-ingestion": "node --require ts-node/register src/scripts/test-map-ingestion.ts",
    "test:discord": "node --require ts-node/register src/scripts/test-discord.ts",
    "register:test": "node --require ts-node/register src/scripts/register-test-commands.ts",
    "create:groups": "ts-node src/scripts/create-default-groups.ts",
    "debug:groups": "ts-node src/scripts/debug-character-groups.ts",
    "fix:groups": "ts-node src/scripts/fix-character-groups.ts",
    "clean:groups": "ts-node src/scripts/clean-character-groups.ts",
    "cleanup-groups": "ts-node src/scripts/cleanup-empty-character-groups.ts",
    "verify:tables": "ts-node src/scripts/verify-tables.ts",
    "remove-character": "ts-node src/interfaces/cli/character-remove.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@types/helmet": "^0.0.48",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.45",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
